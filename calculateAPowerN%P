import java.util.*;
public class MyClass {
    public static void main(String args[]) {
      Scanner sc=new Scanner(System.in);
     int n=sc.nextInt();
     int a=sc.nextInt();
     int p=sc.nextInt();
     long ans=1;
     for(int i=1;i<=n;i++){
         ans=((ans%p)*(a%p))%p;
     }
     System.out.println(ans);
     // System.out.println();
    }
}