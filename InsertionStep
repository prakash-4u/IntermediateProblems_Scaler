import java.util.*;
public class MyClass {
    public static void main(String args[]) {
       int A[]={2,6,10,14,20,4};
       int n=A.length;
       for(int j=n-2;j>=0;j--){
           if(A[j]>A[j+1]){
               int temp=A[j];
               A[j]=A[j+1];
               A[j+1]=temp;
           }
       }
       for(int i:A){
           System.out.print(i+" ");
       }
    }
}