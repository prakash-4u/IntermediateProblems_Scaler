import java.util.*;
public class MyClass {
    public static void main(String args[]) {
        int A[]={2,8,4,-1,6,7,5,10,-1};
      int n=A.length;
      for(int i=0;i<n;i++){
            for(int j=0;j<n-i-1;j++){
                if(A[j]>A[j+1]){
                int temp=A[j];
                A[j]=A[j+1];
                A[j+1]=temp;
            }
        }
      }
        for(int i:A){
            System.out.print(i);
        }
    }
}