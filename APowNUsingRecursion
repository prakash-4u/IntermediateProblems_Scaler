import java.util.*;
public class MyClass {
    public static int power(int a,int n){
        if(n==1){
            return a;
            if(n==0){
                return 1;
            }
        }
        return power(a,n-1)*a;
       
    }
    public static void main(String args[]) {
     Scanner sc=new Scanner(System.in);
     int a=sc.nextInt();
     int n=sc.nextInt();
     int ans=power(a,n);
     System.out.print(ans);
    }
}