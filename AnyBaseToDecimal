import java.util.*;
public class MyClass {
    public static void main(String args[]) {
      int A=1010,B=2;
      int tempSum=1,p=0;
        int value=0;
        while(A>0){
            int temp=A%10;
            tempSum=(int)Math.pow(B,p);
            value+=temp*tempSum;
            p++;
            A=A/10;
        }
      System.out.println(value);
    }
}