import java.util.*;
public class MyClass {
    public static void main(String args[]) {
     Scanner sc=new Scanner(System.in);
     String str=sc.nextLine();
     String str1=sc.nextLine();
     int n=str.length();
     int m=str1.length();
     int i=0;
     while(i<n && i<m){
         if(str.charAt(i)==str1.charAt(i)){
             i++;
         }
         else{
             break;
         }
     }
     System.out.println(i);
    }
}