import java.util.*;
public class MyClass {
    public static void main(String args[]) {
    Integer a[]={1,2,3,4,5};
    int n=a.length;
    Arrays.sort(a,Collections.reverseOrder());
    for(int i=0;i<n;i++){
        System.out.print(a[i]+" ");
    }
    System.out.print(Arrays.toString(a));
    }
}