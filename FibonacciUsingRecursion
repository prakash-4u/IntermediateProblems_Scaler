import java.util.*;
public class MyClass {
    public static int fib(int n){
        if(n==1 || n==2){
            return 1;
        }
        return fib(n-2)+fib(n-1);
    }         
    public static void main(String args[]) {
     Scanner sc=new Scanner(System.in);
     int n=sc.nextInt();
     System.out.print(fib(n));
      //  System.out.print(map);
    }
}