import java.util.*;
public class MyClass {
    public static void main(String args[]) {
      Scanner sc=new Scanner(System.in);
     int n=sc.nextInt();
     int i=sc.nextInt();
     if(((n>>i) & 1) ==1){
         System.out.println("Yes");
     }
     else{
         System.out.println("No");
     }
     // System.out.println();
    }
}