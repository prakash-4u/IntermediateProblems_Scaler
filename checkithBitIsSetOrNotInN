import java.util.*;
public class MyClass {
    public static void main(String args[]) {
      Scanner sc=new Scanner(System.in);
     int n=sc.nextInt();
     int i=sc.nextInt();
     int temp=n&(1<<i);
     if(temp!=0){
         System.out.println("Yes");
     }
     else{
         System.out.println("No");
     }
     // System.out.println();
    }
}