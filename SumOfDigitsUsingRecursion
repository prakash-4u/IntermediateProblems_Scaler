import java.util.*;
public class MyClass {
    public static int sod(int n){
        if(n<10){
            return n;
        }
        return sod(n/10)+(n%10);
       
    }
    public static void main(String args[]) {
     Scanner sc=new Scanner(System.in);
     int n=sc.nextInt();
     int sum=sod(n);
     System.out.print(sum);
    }
}