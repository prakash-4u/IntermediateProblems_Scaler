import java.util.*;
public class MyClass {
    public static void main(String args[]) {
     Scanner sc=new Scanner(System.in);
     int n=sc.nextInt();
    int a[]=new int[n];
    for(int i=0;i<n;i++){
        a[i]=sc.nextInt();
    }
    int max=0,sum=0;
    for(int i=0;i<n;i++){
           sum+=((i+1)*(n-i))*a[i];
        }
        System.out.println(sum);
   //  System.out.println();
    }
}